<Window x:Class="PLAI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{Binding Title, FallbackValue=''}" Height="450" Width="800">
    <StackPanel Margin="16">
        <TextBlock Text="{Binding Title, Mode=OneWay, TargetNullValue=''}" FontSize="16" Margin="0,0,0,8" />
        <Button Content="Click" Width="120" Height="32" />
    </StackPanel>
</Window>
